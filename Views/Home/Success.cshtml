@{
}
@model VPWebSolutions.Models.ContactModel

@{
    ViewData["Title"] = "Contact Us";
}
<h1>Success</h1>

<div>
    <h3>Thank you @Model.FirstName @Model.LastName</h3>
    <hr />
    <p class="message">A copy has been sent to @Model.Email</p>
</div>

<style>
    h1, h3, .message {
        color: #DEC20B;
        font-weight: bold;
    }
</style>